# Based on https://github.com/github/gitignore/blob/master/Android.gitignore

# Gradle files
.gradle/
build/

# Snapshot tests failures (snapshots diffs)
**/out/failures/

# Node files
/scripts/zptbuildtime/node_modules/
/scripts/zptbuildtime/package-lock.json

# Local configuration file (sdk path, etc)
local.properties

# Log Files
*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

# IntelliJ
*.iml
.idea

# External native build folder generated in Android Studio 2.2 and later
.externalNativeBuild

# MacOS
.DS_Store

# Python local modules
dist/
*.egg-info/
venv/
__pycache__
